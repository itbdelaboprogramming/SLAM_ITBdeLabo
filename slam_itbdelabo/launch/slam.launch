<?xml version="1.0"?>
<launch>
    <node pkg="slam_itbdelabo" type="slam_node.py" name="slam_node" respawn="true"></node>
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node" respawn="true">
        <param name="port" value="/dev/ttyUSB0"/>
        <param name="baud" value="57600"/>
    </node>
    <rosparam command="load" file="$(find slam_itbdelabo)/config/slam.yaml" />
</launch>