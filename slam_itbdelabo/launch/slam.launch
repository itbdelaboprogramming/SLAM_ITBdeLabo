<?xml version="1.0"?>
<launch>
    <include file="$(find slam_itbdelabo)/launch/irbot_rplidar.launch"/>
    <node pkg="slam_itbdelabo" type="slam_node.py" name="slam_node" respawn="true" output="screen"></node>
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node" respawn="true" output="screen">
        <param name="port" value="/dev/ttyUSB1"/>
        <param name="baud" value="57600"/>
    </node>
    <rosparam command="load" file="$(find slam_itbdelabo)/config/slam.yaml" />
</launch>